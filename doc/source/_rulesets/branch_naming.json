{
  "id": 2680634,
  "name": "Branch naming",
  "target": "branch",
  "source_type": "Repository",
  "enforcement": "active",
  "conditions": {
    "ref_name": {
      "exclude": [],
      "include": [
        "~ALL"
      ]
    }
  },
  "rules": [
    {
      "type": "deletion"
    },
    {
      "type": "non_fast_forward"
    },
    {
      "type": "branch_name_pattern",
      "parameters": {
        "operator": "regex",
        "pattern": "release\\/*|feat\\/*|maint\\/*|fix\\/*|junk\\/*|no-ci\\/*|dependabot\\/*|test\\/*|testing\\/*|docs\\/*",
        "negate": false,
        "name": ""
      }
    }
  ],
  "bypass_actors": []
}